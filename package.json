{
  "name": "dapper-api",
  "version": "0.1.0",
  "description": "API for Dapper Brew",
  "author": "Dan Brown <dan@danvswild.com>",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist/index.js",
    "test": "eslint src",
    "stage": "now -e DB_HOST=@host -e DB_USER=@user -e DB_PASS=@pass -e JWT_SECRET=@jwtsecret",
    "deploy": "now -e DB_HOST=@host -e DB_USER=@user -e DB_PASS=@pass -e JWT_SECRET=@jwtsecret && now alias"
  },
  "now": {
    "alias": "api.dapperbrew.com"
  },
  "repository": "DapperBrew/dapper-api",
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.4.7",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^4.0.0",
    "es6-promisify": "^5.0.0",
    "express": "^4.13.3",
    "express-validator": "^3.2.1",
    "install": "^0.8.7",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^4.11.4",
    "mongoose-mongodb-errors": "0.0.2",
    "npm": "^4.3.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "resource-router-middleware": "^0.5.1",
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.2",
    "eslint-plugin-import": "^2.2.0",
    "nodemon": "^1.9.2"
  }
}
